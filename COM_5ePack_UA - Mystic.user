    <eval phase="Render" priority="10000"><![CDATA[
      var iLevel as number
      iLevel = #levelcount[Mystic]

      var iDamage as number
      var sDamage as string

      if (iLevel >= 14) then
        iDamage = 2
      else
        iDamage = 1
      endif

      field[abValue].value = iDamage]]></eval>
    <eval phase="PostAttr" priority="10000" index="2">hero.child[c5CMyPP].field[abValue3].value = hero.child[aINT].field[aModBonus].value
        field[abValue3].value = hero.child[c5CMyPP].field[abValue3].value</eval>
