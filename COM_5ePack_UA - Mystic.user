    <eval phase="PreLevel" priority="10000"><![CDATA[   var sText as string
    var iPPMax as number
    if (#levelcount[Mystic] >= 9) then
      iPPMax = 7
    elseif (#levelcount[Mystic] >= 7) then
      iPPMax = 6
    elseif (#levelcount[Mystic] >= 5) then
      iPPMax = 5
    elseif (#levelcount[Mystic] >= 3) then
      iPPMax = 3
    else
      iPPMax = 2
    endif

    #trkmax[trk5CPsiMx] = iPPMax]]></eval>
    </thing>
