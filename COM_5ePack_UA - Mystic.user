  <thing id="c5CImmDur" name="Immortal Durability" description="At 1st level your HP max increases by 1 per mystic level.\n\nIn addition when you are not wearing armor or using a shield, your base AC equals 10 + Dex + Con mods." compset="ClSpecial">
    <usesource source="Mystic_v3"/>
    <tag group="AddReq" tag="ArmorNone" name="Cannotbe used in armor"/>
    <tag group="AddReq" tag="ArmorNoSh" name="Cannot be used with a shield"/>
    <eval phase="PostAttr" priority="10000"><![CDATA[
      ~ If we're not shown, just get out now
~      doneif (tagis[Helper.ShowSpec] = 0)

~      doneif (tagis[Helper.Disable] <> 0)
      
      field[abValue2].value += #attrmod[aCON]

      hero.child[ArmorClass].field[tACUnarDef].value = maximum(hero.child[ArmorClass].field[tACUnarDef].value,field[abValue2].value)]]></eval>
    <eval phase="PostLevel" priority="5000" index="2">field[abValue].value += maximum(herofield[tHitDice].value, 1)</eval>
    <eval phase="PostLevel" priority="15000" index="3">herofield[tBonusHP].value += field[abValue].value</eval>
    </thing>
  <thing id="c5CPsiRes" name="Psionic Resilience" description="At 3rd level, your psionic ability gives you temporary hit points equal to your Intelligence modifier (min 0) as long as you have 1 or more hit points." compset="ClSpecial" summary="Gain temporary hit points equal to your Intelligence modifier (min 0) as long as you have 1 or more hit points.">
    <usesource source="Mystic_v3"/>
    <tag group="abRange" tag="Personal"/>
    <bootstrap thing="th5CPsiRes">
      <assignval field="abilActive" value="1"/>
      </bootstrap>
    <eval phase="Final" priority="10000"><![CDATA[
      ~ If we're not shown, just get out now
      doneif (tagis[Helper.ShowSpec] = 0)

      ~ If we're disabled, do nothing
      doneif (tagis[Helper.Disable] <> 0)

      ~ If current hit points are noy greater than 0
      ~doneif (herofield[tCurrentHP].value < 1)

     
      field[abValue].value += #attrmod[aINT]
      
      var temphp as number
      temphp = field[abValue].value

      hero.childfound[th5CPsiRes].field[thpMinAllw].value = temphp
      hero.childfound[th5CPsiRes].field[thpMaxAllw].value = temphp]]></eval>
    </thing>
  <thing id="c5CSrgOHlt" name="Surge of Health" description="At 6th level, you can use your psychic focus to escape death&apos;s grasp. As a reaction, when you take damage you may halve that damage against you. Your focus immediately ends if it is active, you cannot reactivate your focus until you finish a short or long rest.\n\nYou may not use this feature if you cannot use your focus." compset="ClSpecial">
    <usesource source="Mystic_v3"/>
    </thing>
  <thing id="c5CImmrtWl" name="Immortal Will" description="At 14th level, you may draw upon your reserves of psychic power to survive beyond death. At the end of your turn, while at 0 HP you may spend 5 psi points to immediately regain a number of HP equal to your Mystic level + your Con modifier." compset="ClSpecial">
