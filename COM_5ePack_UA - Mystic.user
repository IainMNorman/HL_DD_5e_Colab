    <fieldval field="usrCandid1" value="component.BaseSkill &amp; (Reference.skAnimHand | Reference.skArcana | Reference.skHistory | Reference.skInsight | Reference.skMedicine| Reference.skNature | Reference.skPercep | Reference.skReligion | Reference.skSurvival)  &amp; !Helper.Proficient"/>
    <fieldval field="usrCandid2" value="component.BaseSkill &amp; (Reference.skAnimHand | Reference.skArcana | Reference.skHistory | Reference.skInsight | Reference.skMedicine| Reference.skNature | Reference.skPercep | Reference.skReligion | Reference.skSurvival)  &amp; !Helper.Proficient"/>
