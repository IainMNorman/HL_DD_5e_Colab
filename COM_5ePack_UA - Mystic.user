  <thing id="ta5cPsHamm" name="Psychic Hammer" description="As an action, you try to grasp one creature you can see within 120 feet of you, with a hand crafted from telekinetic energy. The target must succeed on a {b}Strength saving throw{/b} or take 1d6 force damage. If it takes any of this  damage and is Large or smaller, you can move it up to 10 feet in a straight line in a direction of your choice.\nYou can’t lift the target off the ground unless it is already airborne or underwater.\nThe talent’s damage increases by 1d6 when you reach 5th level (2d6), 11th level (3d6), and 17th level (4d6)." compset="CustomSpec" summary="You grasp a creature and do damage to it" uniqueness="unique">
    <usesource source="Mystic_v3" parent="p5eUneArc" name="5e Unearthed Arcana - Mystic v3"/>
    <tag group="Helper" tag="Tertiary" name="Tertiary" abbrev="Tertiary"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="Custom" tag="5CPsiTDam"/>
    <tag group="Helper" tag="SingleAtt"/>
    <tag group="DamageType" tag="dtForce"/>
    <tag group="SpecSource" tag="cHelp5CMY3" name="Mystic UA" abbrev="Mystic UA"/>
    <tag group="Helper" tag="NoAttrDam"/>
    <bootstrap thing="taPo5cPsHa">
      <autotag group="Helper" tag="PsiLike"/>
      <autotag group="Helper" tag="SpellLike"/>
      </bootstrap>
    <eval phase="Final" priority="12000"><![CDATA[
      var iLevel as number
      iLevel = #totallevelcount[]
      var iDice as number
      var sDamage as string
      var iBonus as number

      if (iLevel >= 17) then
        iDice = 4
      elseif (iLevel >= 11) then
        iDice = 3
      elseif (iLevel >= 5) then
        iDice = 2
      else
        iDice = 1
      endif

      sDamage = iDice & "d6"

      ~ Talent damage bonus 
      if (hero.child[c5CMyPP].field[abValue3].value > 0) then
        iBonus = hero.child[c5CMyPP].field[abValue3].value
      endif

      if (iBonus > 0) then
        sDamage &= "+" & iBonus
      endif

      sDamage &= " force"

      field[wFixRanDam].text = sDamage

      field[livename].text = field[thingname].text & " (Damage: " & sDamage & ")"]]></eval>
    </thing>
