    <tag group="abCategory" tag="5COrdNomad"/>
    </thing>
  <thing id="c5CSupTele" name="Superior Teleportation" description="Upon reaching 6th level, you gain the superior ability for teleportation. When you use a discipline to teleport, you may increase that distance by up to 10 ft." compset="ClSpecial">
    <usesource source="Mystic_v3"/>
    <tag group="abCategory" tag="5COrdNomad"/>
    </thing>
  <thing id="c5CEffJour" name="Effortless Journey" description="At 14th level, your mind gains the ability to mystically move your body. Once per turn, you can move up to 30 ft via teleportation. You muist beable to see the unoccupied space to teleport in to it." compset="ClSpecial">
    <usesource source="Mystic_v3"/>
    <tag group="abCategory" tag="5COrdNomad"/>
    </thing>
  <thing id="cfg5CAvat" name="Avatar Abilities" compset="Configure" uniqueness="unique">
